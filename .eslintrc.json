{
    "extends": ["airbnb-base"],
    "rules": {
      "linebreak-style": ["error", "unix"],
      "max-len": ["error", { "code": 120 }],
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
      "simple-import-sort/sort": "error"
    },
    "plugins": [
      "simple-import-sort"
    ]
}
